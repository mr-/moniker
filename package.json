{
  "name": "moniker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-node6": "^11.0.0",
    "body-parser": "^1.18.2",
    "bootstrap": "3",
    "cookie-parser": "^1.4.3",
    "google-auth-library": "^0.11.0",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "node-sass-chokidar": "^0.0.3",
    "promise": "^8.0.1",
    "q": "^1.5.0",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.6.1",
    "react-google-login": "^2.9.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.13",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sqlite3": "^3.1.12",
    "universal-cookie": "^2.1.0"
  },
  "scripts": {
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "server": "react-scripts build && ./node_modules/.bin/babel-node --presets node6 ./app.js",
    "start-js": "react-scripts start",
    "start": "./node_modules/.bin/babel-node --presets node6 ./app.js",
    "oldstart": "npm-run-all -p watch-css start-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
